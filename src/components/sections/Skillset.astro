---
import Skill from "../Skill.astro";

const Languages = [
  {
    title: "Java",
    icon: "data:image/png;base64,",
    href: "https://www.java.com/en/",
    tooltip: "/images/screenshots/java.jpg",
  },
  {
    title: "C",
    icon: "data:image/png;base64,",
    href: "https://www.cprogramming.com/",
    tooltip: "/images/screenshots/C.jpg",
  },
];

const Web = [
  {
    title: "NextJS",
    icon: "/images/skills/nextjs.png",
    href: "https://nextjs.org",
    tooltip: "/images/screenshots/nextjs.png",
  },

  {
    title: "TypeScript",
    icon: "/images/skills/typescript.png",
    href: "https://typescriptlang.org",
    tooltip: "/images/screenshots/typescript.png",
  },
  {
    title: "tailwindcss",
    icon: "/images/skills/tailwind.png",
    href: "https://tailwindcss.com",
    tooltip: "/images/screenshots/tailwind.png",
  },
  {
    title: "ReactJS",
    icon: "data:image/png;base64,",
    href: "https://reactjs.com",
    tooltip: "/images/screenshots/react.jpg",
  },
];

const backend = [
  {
    title: "NodeJs",
    icon: "data:image/png;base64,",
    href: "https://nodejs.org/en",
    tooltip: "/images/screenshots/nodejs.jpg",
  },

  {
    title: "ExpressJS",
    icon: "data:image/png;base64,",
    href: "https://expressjs.com/",
    tooltip: "/images/screenshots/express.jpg",
  },
  {
    title: "MongoDb",
    icon: "data:image/png;base64,",
    href: "https://www.mongodb.com/",
    tooltip: "/images/screenshots/mongodb.jpg",
  },
  {
    title: "Redis",
    icon: "data:image/png;base64,",
    href: "https://redis.io/",
    tooltip: "/images/screenshots/redis.jpg",
  },
  {
    title: "PostgreSql",
    icon: "data:image/png;base64,",
    href: "https://www.postgresql.org/",
    tooltip: "/images/screenshots/postgres.jpg",
  },
  {
    title: "MySql",
    icon: "data:image/png;base64,",
    href: "https://www.mysql.com/",
    tooltip: "/images/screenshots/mysql.jpg",
  },
  {
    title: "GraphQL",
    icon: "data:image/png;base64,",
    href: "https://graphql.org/",
    tooltip: "/images/screenshots/graphql.jpg",
  },
];
const Devops = [
  {
    title: "AWS",
    icon: "data:image/png;base64,",
    href: "https://aws.amazon.com/",
    tooltip: "/images/screenshots/aws.jpg",
  },
  {
    title: "C",
    icon: "data:image/jpeg;base64,",
    href: "https://www.docker.com/",
    tooltip: "/images/screenshots/docker.jpg",
  },
];
---

<section class="skillsets">
  <div>
    <h1 class="title">Technical Skills</h1>
    <div class="skilldivider">
      <div class="row">
        <span>Programming Languages</span>
        <div class="row">
          {Languages.map((lang) => <Skill {...lang} />)}
        </div>
      </div>
      <div class="row">
        <span>Web dev - frontend</span>
        <div class="row">
          {Web.map((webop) => <Skill {...webop} />)}
        </div>
      </div>
      <div class="row">
        <span>Web dev - Backend</span>
        <div class="row">
          {backend.map((back) => <Skill {...back} />)}
        </div>
      </div>
      <div class="row">
        <span>Cloud and Devops</span>
        <div class="row">
          {Devops.map((dev) => <Skill {...dev} />)}
        </div>
      </div>
    </div>
  </div>

  <style>
    .row {
      display: flex;
      gap: 10px;
      flex-wrap: wrap;
    }

    .skilldivider {
      display: flex;
      flex-direction: column;
      gap: 20px;
    }

    .skillsets {
      display: flex;
      flex-direction: column;
      gap: 20px;
      max-width: 270px;
    }
  </style>
</section>
